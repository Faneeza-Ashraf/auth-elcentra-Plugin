<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/reporting/db" VERSION="20160518" COMMENT="XMLDB file for Moodle blocks/reporting"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="reporting_filter" COMMENT="block reporting filter">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="user_info_field_id" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="user info field id"/>
        <FIELD NAME="status" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT="status"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="general_filter" COMMENT="block general filter">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="filtername" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="general filter name"/>
        <FIELD NAME="status" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT="status"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="report_vendor_user" COMMENT="Report vendor user">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="vendorname" TYPE="char" LENGTH="256" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courselist" TYPE="char" LENGTH="128" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="timecreated"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="report_setting" COMMENT="Default comment for the table, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false" COMMENT="name of setting"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Description"/>
        <FIELD NAME="setting" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false" COMMENT="Setting such as color or config"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="timecreated"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>